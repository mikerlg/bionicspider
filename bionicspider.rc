        MENUITEM "M&inimize",                   ID_WINDOW_MINIMIZE
        MENUITEM "Rest&ore",                    ID_WINDOW_RESTORE
        MENUITEM SEPARATOR
        MENUITEM "Minimize to &tray",           ID_WINDOW_MINIMIZETOTRAY
        MENUITEM "A&lways on top",              ID_WINDOW_ALWAYSONTOP
        MENUITEM SEPARATOR
        POPUP "&Positioning"
        BEGIN
            MENUITEM "M&emory",                     ID_POSITIONING_MEMORY
            MENUITEM "Rec&all",                     ID_POSITIONING_RECALL
        END
        POPUP "E&xplorer"
        BEGIN
            MENUITEM "Conso&le",                    ID_EXPLORER_CONSOLE
            MENUITEM "&Clone",                      ID_EXPLORER_CLONE
        END
        MENUITEM SEPARATOR
        MENUITEM "&Close",                      ID_WINDOW_CLOSE
        MENUITEM SEPARATOR
        MENUITEM "R&ename...",                  ID_WINDOW_RENAME
        MENUITEM "&Remove",                     ID_WINDOW_REMOVE
