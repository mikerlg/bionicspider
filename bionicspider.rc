        BEGIN
            MENUITEM "&Group memory\tCtrl+M",       ID_GROUPPOSITIONS_GROUPMEMORY

            MENUITEM "Reca&ll all\tAlt+M",          ID_GROUPPOSITIONS_RECALLALL

        END
        MENUITEM SEPARATOR
        MENUITEM "&Add Window By Title...",     ID_GROUP_ADDWINDOWBYTITLE
        MENUITEM SEPARATOR
        MENUITEM "&Sort Group",                 ID_GROUP_SORTALLWINDOWSINGROUP

