        MENUITEM "Remo&ve windows",             ID_GROUP_REMOVEALLWINDOWSINGROUP

        MENUITEM "&Remove Group",               ID_GROUP_REMOVETHISGROUP
        MENUITEM "R&ename Group...",            ID_GROUP_RENAMETHISGROUP
    END
    POPUP "&Window"
    BEGIN
        MENUITEM "&Hide\tCtrl+L",               ID_WINDOW_HIDEWINDOW
        MENUITEM "&Show\tCtrl+K",               ID_WINDOW_SHOWWINDOW
