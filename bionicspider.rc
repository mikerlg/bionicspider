        MENUITEM "&Maximize Group\tAlt+A",      ID_GROUP_MAXIMIZEALLWINDOWS
        MENUITEM "M&inimize Group\tAlt+S",      ID_GROUP_MINIMIZEALLWINDOWS
        MENUITEM "Rest&ore Group\tAlt+D",       ID_GROUP_RESTOREALLWINDOWS
        MENUITEM SEPARATOR
